@model List<EmployeeViewModel>

@{
    ViewData["Title"] = "Employee List";
}

<h1>Employee List</h1>

<table class="table">
    <thead>
    <tr>
        <th>Employee ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Job Name</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var employee in Model)
    {
        <tr>
            <td>@employee.EmployeeId</td>
            <td>@employee.FirstName</td>
            <td>@employee.LastName</td>
            <td>@employee.Email</td>
            <td>@employee.JobName</td>
        </tr>
    }
    </tbody>
</table>